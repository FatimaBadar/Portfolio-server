{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/getProjects",
      "dest": "/api/getProjects.js"
    },
    {
      "src": "/api/contactMe",
      "dest": "/api/contactMe.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 10
    }
  }
}
