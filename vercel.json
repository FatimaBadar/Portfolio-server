{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 4
    }
  },
  "routes": [
    {
      "src": "/api/getProjects",
      "dest": "/api/getProjects.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
